<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:paddingLeft="@dimen/size_15dp"
    android:paddingRight="@dimen/size_15dp"
    android:paddingTop="@dimen/size_15dp"
    android:orientation="vertical">
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:background="@drawable/ic_number_plate"
            android:id="@+id/tv_car_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/white"
            android:layout_marginStart="@dimen/size_2dp"
            android:text="京A 8888 "/>
    <ImageView
        android:id="@+id/iv_car"
        android:layout_width="@dimen/size_164dp"
        android:layout_height="@dimen/size_164dp" />
    </RelativeLayout>
    <TextView
        android:id="@+id/tv_sxt"
        android:layout_marginStart="@dimen/size_2dp"
        android:layout_marginTop="@dimen/size_4dp"
        android:layout_width="@dimen/size_98dp"
        android:layout_height="@dimen/size_20dp"
        android:textSize="@dimen/text_size_14sp"
        android:textColor="@color/black_common"
        style="@style/CommonTextItem"
        android:text="谱广南门摄像头"/>
    <TextView
        android:id="@+id/tv_date"
        android:layout_marginStart="@dimen/size_2dp"
        android:layout_marginTop="@dimen/size_4dp"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/size_17dp"
        android:textSize="@dimen/text_size_12sp"
        android:textColor="@color/color_989FB0"
        style="@style/CommonTextItem"
        android:text="谱广南门摄像头"/>
</LinearLayout>