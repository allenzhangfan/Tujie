<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/color_FFFDF9F2" />
            <stroke android:width="0.5dp" android:color="@color/app_theme_color" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <solid android:color="@color/color_EEEEEE" />
            <stroke android:width="0.5dp" android:color="@color/color_E5E6EB" />
        </shape>
    </item>
</selector>